{
  "version": "2.4.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://willikay11.github.io/solgates-component-library",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build && yarn build-tailwind",
    "test": "dts test --passWithNoTests",
    "lint": "dts lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --docs",
    "preview-storybook-docs": "storybook dev --docs",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify",
    "lintfix": "yarn run lint -- --fix",
    "format": "prettier --write \"./src/**/*\" \"./stories/**/*.tsx\""
  },
  "publishConfig": {
    "@willikay11:registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@willikay11/solgates-component-library",
  "author": "William Kamau",
  "module": "dist/solgates-component-library.esm.js",
  "size-limit": [
    {
      "path": "dist/solgates-component-library.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/solgates-component-library.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.4.5",
    "@storybook/builder-vite": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "dts-cli": "^2.0.3",
    "eslint-plugin-storybook": "^0.6.14",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "storybook": "^7.4.5",
    "tailwindcss": "^3.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/preset-react": "^7.22.15",
    "@cloudinary/base": "^1.0.0-beta.4",
    "@cloudinary/react": "^1.11.2",
    "@headlessui/react": "^1.7.7",
    "@popperjs/core": "^2.11.7",
    "@storybook/react-vite": "^7.4.5",
    "@tanstack/react-table": "^8.7.6",
    "react-popper": "^2.3.0",
    "react-swipeable": "^7.0.1",
    "vite": "^4.4.9"
  },
  "packageManager": "yarn@3.6.3"
}
